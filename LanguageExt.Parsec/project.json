{
  "version": "1.9.1",
  "authors": [ "Paul Louth" ],
  "description": "Parser combinators library based on Haskell Parsec.  This is part of the LanguageExt functional base class library and requires LanguageExt.Core",

  "packOptions": {
    "title": "LanguageExt.Parsec",
    "owners": [ "paullouth" ],
    "tags": [ "C#", "Functional", "Language Extension", "Monad", "Option", "Either", "Reader", "Writer", "State", "List", "Set", "Map", "Queue", "Memo", "Memoization", "Immutable", "Lambda", "Pattern Matching", "Tuple", "Actor", "Process", "Akka", "Redis", "Parser", "Combinators", "Parser combinators" ],
    "iconUrl": "https://camo.githubusercontent.com/08525ae465a9061150679d871731e77b399c2a94/687474703a2f2f7777772e34666f75722e6f72672f696d616765732f6c616e672d6578742d6c6f676f2e706e67",
    "releaseNotes": "",
    "projectUrl": "https://github.com/louthy/language-ext",
    "licenseUrl": "https://github.com/louthy/language-ext/blob/master/LICENSE.md"
  },

  "copyright": "Copyright (c) Paul Louth",
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ],
        "compile": "*.cs"
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true,
        "compile": "*.cs"
      }
    }
  },
  "dependencies": {
    "LanguageExt.Core": {
      "version": "1.9.1",
      "target": "project"
    },    
    "System.Reactive.Core": "3.0.0-*",
    "System.Reactive.Interfaces": "3.0.0-*",
    "System.Reactive.PlatformServices": "3.0.0-*",
    "System.Reactive.Linq": "3.0.0-*"
  },
  "frameworks": {
    "net45": {
    },
    "net46": {
    },
    "netstandard16": {
      "buildOptions": { "define": [ "COREFX" ] },
      "dependencies": {
        "System.Security.Cryptography.Algorithms": "4.2.0",
        "System.Collections": "4.0.11",
        "System.Collections.Concurrent": "4.0.12",
        "System.Linq.Queryable": "4.0.1"
      }
    }
  }
